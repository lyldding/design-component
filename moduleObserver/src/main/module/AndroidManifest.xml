<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lyldding.moduleobserver">

    <application
        android:name="debug.ModuleApplication"
        android:allowBackup="true"
        android:icon="@mipmap/observe_ic_launcher"
        android:label="@string/observe_app_name"
        android:roundIcon="@mipmap/observe_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ObserveMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>